{
    "primary_language": "en",
    "debugging_mode": true,
    "router": [
        {"path": "/", "page": "index"},
        {"path": "/support", "page": "support"},
        {"path": "/support/about", "page": "about"},
        {"path": "/servers", "page": "servers"},
        {"path": "/servers/view", "page": "server"},
        {"path": "/api", "page": "api"},
        {"path": "/auth/login_required", "page": "login_required"},
        {"path": "/auth/logout", "page": "logout"}
    ],
    "libraries": {
        "IntellivoidAccounts": {
            "directory_name": "IntellivoidAccounts",
            "autoloader": "IntellivoidAccounts.php",
            "check_class_exists": true,
            "namespace": "IntellivoidAccounts",
            "main_class": "IntellivoidAccounts"
        },
        "Logixal": {
            "directory_name": "Logixal",
            "autoloader": "Logixal.php",
            "check_class_exists": true,
            "namespace": "Logixal",
            "main_class": "Logixal"
        },
        "ModularAPI": {
            "directory_name": "ModularAPI",
            "autoloader": "ModularAPI.php",
            "check_class_exists": true,
            "namespace": "ModularAPI",
            "main_class": "ModularAPI"
        },
        "OpenBlu": {
            "directory_name": "OpenBlu",
            "autoloader": "OpenBlu.php",
            "check_class_exists": true,
            "namespace": "OpenBlu",
            "main_class": "OpenBlu"
        },
        "SecuredWebSessions": {
            "directory_name": "sws",
            "autoloader": "sws.php",
            "check_class_exists": true,
            "namespace": "sws",
            "main_class": "sws"
        },
        "Support": {
            "directory_name": "Support",
            "autoloader": "Support.php",
            "check_class_exists": true,
            "namespace": "Support",
            "main_class": "Support"
        },
        "COASniffle": {
            "directory_name": "COASniffle",
            "autoloader": "COASniffle.php",
            "check_class_exists": true,
            "namespace": "COASniffle",
            "main_class": "COASniffle"
        }
    },
    "runtime_scripts": {
        "initialize": ["coa_sniffles.php"],
        "on_page_load": ["client_mode.php", "check_auth.php", "cache.php"],
        "page_loaded": [],
        "after_request": [],
        "on_request": [],
        "on_import": []
    }
}